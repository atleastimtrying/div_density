{
  "manifest_version": 2,
  "name": "Div Density",
  "description": "This extension shows the density of elements on your page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["app.js"],
    "persistent": false
  }
}
